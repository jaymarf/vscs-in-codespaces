{
	"$schema": "https://raw.githubusercontent.com/microsoft/vscode/master/extensions/configuration-editing/schemas/devContainer.schema.json",
	"name": "Codespaces development in GitHub Codespaces",
	"settings": {
		"terminal.integrated.shell.linux": "/bin/bash",
		"omnisharp.defaultLaunchSolution": "VSOnline.sln",
		"omnisharp.projectLoadTimeout": 300,
		"remote.portsAttributes": {
			"59330": {
				"label": "Diagnostics Server",
				"onAutoForward": "openBrowser"
			},
			"53760": {
				"label": "Frontend Service"
			},
			"59228": {
				"label": "Backend Service"
			}
		}
	},
	"extensions": [
		"ms-dotnettools.csharp"
	],
	"dockerFile" : "Dockerfile",
	"containerEnv": {
		"VSCLK_REPO_URL": "https://devdiv.visualstudio.com/DefaultCollection/OnlineServices/_git/vsclk-core",
		"CASCADE_REPO_URL": "https://devdiv.visualstudio.com/DefaultCollection/DevDiv/_git/Cascade"
	},
	/*
	 * Please don't change! create your own `post-create-command.sh`
	 * file in this folder instead, it will be called automatically
	 * once the Codespace is fully set up.
	 */
	"postCreateCommand": "/bin/bash .codespaces/post-create.sh"
}
